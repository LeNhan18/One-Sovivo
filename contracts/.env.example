# Sovico Passport NFT Smart Contract Environment Variables

# Private key for deployment (without 0x prefix)
# WARNING: Never commit this file with real private keys!
PRIVATE_KEY=your_private_key_here

# API Keys for contract verification
BSCSCAN_API_KEY=your_bscscan_api_key_here
POLYGONSCAN_API_KEY=your_polygonscan_api_key_here
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Gas reporting
REPORT_GAS=false

# Network URLs (optional, using public RPCs by default)
BSC_TESTNET_URL=https://data-seed-prebsc-1-s1.binance.org:8545/
BSC_MAINNET_URL=https://bsc-dataseed1.binance.org/
POLYGON_MUMBAI_URL=https://rpc-mumbai.maticvigil.com/
POLYGON_MAINNET_URL=https://polygon-rpc.com/

# Deployment configuration
INITIAL_OWNER=your_initial_owner_address_here
BASE_TOKEN_URI=https://api.sovico.vn/passport/metadata/

